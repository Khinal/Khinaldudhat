package s00__assignmentModual2;

import java.util.ArrayList;
import java.util.List;

public class QES__43 {
	
	public static void main(String[] args) {
		
		 List<String> l = new ArrayList<>();
	      l.add("1");
	      l.add("2");
	      l.add("3");
	      System.out.println("List: " + l);
	      String student = "1";
	      String missingStudent = "4";
	      System.out.println("1 is present at: " + l.indexOf(student));
	      System.out.println("4 index: " + l.indexOf(missingStudent));
	}

}
