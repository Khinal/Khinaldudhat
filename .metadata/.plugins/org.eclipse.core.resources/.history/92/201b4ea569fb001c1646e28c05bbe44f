package OOPS;

class Methoddemo
{
	public void display() {
		
		System.out.println("runing dispaly method....");
	}
	
	public void add(int a, int b) {
		
		int result = a+b;
		System.out.println("Result is : "+result);
	}

	public class MethodManipulation {
	
	
		public static void main(String[] args) {
		
		Methoddemo md = new Methoddemo();
		md.display();
		md.add(10, 20);
		
		}
	

}
     }

