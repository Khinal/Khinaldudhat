package s00__assignmentModual2;

import java.util.ArrayList;
import java.util.List;

public class QES__43 {
	
	public static void main(String[] args) {
		
		 List<String> l = new ArrayList<>();
	      l.add("A");
	      l.add("B");
	      l.add("C");
	      System.out.println("List: " + l);
	      String student = "B";
	      String missingStudent = "E";
	      System.out.println("A is present at: " + l.indexOf(student));
	      System.out.println("E index: " + l.indexOf(missingStudent));
	}

}
