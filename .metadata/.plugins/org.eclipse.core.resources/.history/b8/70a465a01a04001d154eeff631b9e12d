package string;

public class Stringprecticle {
	
	public static void main(String[] args) {
		
		String str = "sun rises in east";
		
		//east in rieses sun
		
		String ar [] = str.split(" ");
		for(int i=ar.length-1; i>=0; i--)
		{
			System.out.print(ar[i]+" ");
		}
		 
		
	
	
		
	
	
	
	
	
	
	}
	
	
	
	
	
	
	

}
