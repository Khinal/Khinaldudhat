package string;

public class Stringprecticle {
	
	public static void main(String[] args) {
		
		
		String str = "sun rise in east";
		
		String ar [] = str.split(" ");
		for(int i=ar.length-1; i>=0; i--)
		{
			System.out.print(ar[i]+" ");
		}
		System.out.println();
		System.out.println("********************************");
		
		 char ch [] = str.toCharArray();
		 for(int i=ch.length-1; i>=0;)
		 {
			 System.out.print(ch[i]);
			 
		 }
		 System.out.println();
		 System.out.println("******************************");
		 
		 
		
		
		 
		 
		 
		
	}

}
