
public class ArrayCopy {

	public static void main(String[] args) {
		
		
		
		
		int a[] = {10,20,30,40,50,60};
		int a1[] = {20,30,40,50,60};	
		int b[] = new int[a.length+a1.length];
		
		for (int i = 0; i<b.length; i++) 
		{
			b[i] = a1[i];
		}
		
		
		{
			System.out.println(b[i]);
			
			
			
		
	}
	}
}
